    "deploy": "wrangler deploy",
    "test": "vitest",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@cloudflare/workers-types": "^4.20241022.0",
    "miniflare": "^4.20251011.1",
    "typescript": "^5.6.3",
    "vitest": "^4.0.5",
    "wrangler": "^4.45.3"
  }
