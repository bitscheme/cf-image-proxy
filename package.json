{
  "name": "cf-image-proxy",
  "private": true,
  "description": "Proxy CDN for caching static assets from third-party domains via Cloudflare Workers.",
  "author": {
    "name": "James Holcomb"
  },
  "repository": "bitscheme/cf-image-proxy",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "wrangler dev",
    "preview": "wrangler dev --remote",
